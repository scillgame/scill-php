{
    "name": "scill/example-send-event-php",
    "description": "A simple example on how to send an event",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "https://www.scillgame.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Phillip Schuster",
            "email": "phillip@4players.de",
            "homepage": "https://www.4playershq.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../lib"
        }
    ],
    "require": {
        "php": "^7.2",
        "scillgame/scill-php": "dev-master"
    },
    "require-dev": {
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
